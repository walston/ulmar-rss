{
  "name": "ulmar-rss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "**** SETUP ****": "",
    "setup": "npm run new-git-hook && npm run new-env",
    "new-git-hook": "if [[ ! -a ./.git/hooks/pre-push ]]; then cp ./pre-push ./.git/hooks/pre-push; fi;",
    "new-env": "if [[ ! -a ./.env ]]; then cp .env.sample .env; fi;",
    "**** PROCESS ****": "",
    "sync": "source .env; rsync -azP ./audio/ $HOST:$PATH_TO_AUDIO_STORAGE/audio",
    "build": "node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "rss": "^1.2.2"
  }
}
