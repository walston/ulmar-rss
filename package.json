{
  "name": "ulmar-rss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "|||||||| LOCAL SETUP ||||||||": "",
    "setup-local": "npm run setup-pre-push && npm run setup-local-env",
    "setup-pre-push": "if [[ ! -a ./.git/hooks/pre-push ]]; then cp hooks/pre-push .git/hooks/pre-push; fi;",
    "setup-local-env": "if [[ ! -a ./.env ]]; then cp .env.sample .env; fi;",
    "|||||||| HOST SETUP ||||||||": "",
    "setup-post-receive": "if [[ ! -a ./.git/hooks/post-receive ]]; then cp hooks/post-receive .git/hooks/post-receive; fi;",
    "|||||||| PROCESS ||||||||": "",
    "sync": "source .env; rsync -azP ./audio/ $HOST:$PATH_TO_AUDIO_STORAGE/audio",
    "build": "node index.js",
    "postbuild": "source .env; rsync -azP ./dist/ $SERVER_PATH"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "rss": "^1.2.2"
  }
}
